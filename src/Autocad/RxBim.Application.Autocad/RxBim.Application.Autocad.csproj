<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net472</TargetFramework>
        <PlatformTarget>x64</PlatformTarget>
        <NoWarn>NU1701</NoWarn>
        <AssemblyName>$(MSBuildProjectName).$(ApiVersion)</AssemblyName>
        <Configurations>Debug;Release</Configurations>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="AutoCAD2019.Base" Version="1.0.3" ExcludeAssets="build; runtime" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\RxBim.Command.Generators.Autocad\RxBim.Command.Generators.Autocad.csproj" />
      <ProjectReference Include="..\..\Core\RxBim.Shared\RxBim.Shared.csproj" />
    </ItemGroup>

    <PropertyGroup>
        <Version>$(ApiVersion)</Version>
         <Description>RxBim Application api Library for AutoCAD</Description>
        <PackageId>RxBim.Application.Autocad</PackageId>
    </PropertyGroup>

</Project>
