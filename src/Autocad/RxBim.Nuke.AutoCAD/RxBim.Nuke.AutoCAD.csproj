<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <PlatformTarget>x64</PlatformTarget>
        <NoWarn>NU1701;NU1702</NoWarn>
        <AssemblyName>$(MSBuildProjectName).$(ApiVersion)</AssemblyName>
        <Configurations>Debug;Release</Configurations>
    </PropertyGroup>

    <ItemGroup>
      <ProjectReference Include="..\..\Core\RxBim.Nuke\RxBim.Nuke.csproj" />
    </ItemGroup>

    <PropertyGroup>
        <Version>$(ApiVersion)</Version>
         <Description>RxBim nuke plugin for building MSI for AutoCAD</Description>
        <PackageId>RxBim.Nuke.AutoCAD</PackageId>
    </PropertyGroup>

</Project>
